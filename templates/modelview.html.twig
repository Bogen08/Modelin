{# templates/modelview.html.twig #}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('model') }}
    {% endblock %}
    <script>
        var id={{ post_id }}
    </script>

</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    {{ encore_entry_script_tags('model') }}
    <script src="https://kit.fontawesome.com/0dcb25cdf9.js" crossorigin="anonymous"></script>
    <script type="text/javascript" src="{{asset('js/imgSwitch.js')}}" defer></script>
{% endblock %}
</body>
</html>